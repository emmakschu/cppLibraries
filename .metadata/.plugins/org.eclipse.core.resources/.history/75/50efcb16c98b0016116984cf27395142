 * A custom C++ library for performing basic descriptive
 * statistical calculations. Will be Mirrored by equivalent
 * libraries for Python3 and Java
 */

double length(double myArray[])
{
	double i = 0;
	double max = sizeof(myArray);

	while (i < max)
	{
		i++;
	}

	return i;
}

double arraySum(double myArray[])
{
	double sum = 0;
	int i;

	for (i = 0; i <= sizeof(myArray); i++)
	{
		sum += myArray[i];
	}

	return sum;
}
