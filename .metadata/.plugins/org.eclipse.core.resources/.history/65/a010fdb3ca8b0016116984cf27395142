 * A custom C++ library for performing basic descriptive
 * statistical calculations. Will be Mirrored by equivalent
 * libraries for Python3 and Java
 */

#include <vector>

double length(std::vector<double> myArray[])
{
	double i;
	i = myArray.size();
	return i;
}

double arraySum(std::vector<double> myArray[])
{
	double sum = 0;
	int i;

	for (i = 0; i <= myArray.size(); i++)
	{
		sum += myArray[i];
	}

	return sum;
}
